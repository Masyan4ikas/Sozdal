// Дано число A. Вычислить A8 , используя вспомогательную переменную и три операции умножения.

#include <iostream>
#include <math.h>
using namespace std;

int main() 
{
    setlocale(0, "rus");
    double a =0 , b = 0;
    cout << "Найдем    A^8\n";
    cout << "Введите     A: \n";
    cout << "A = ";
    cin >> a;
    b = a * a;
    b = b * b;
    b = b * b;
    cout << "A^8 = " << b;
}
