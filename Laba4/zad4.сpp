------------------------------------------------------------------------------------------------------------
/*Даны два ненулевых числа. Найти сумму, разность, произведение и
частное их квадратов.
*/
------------------------------------------------------------------------------------------------------------

1-ый Пример решения
#include <iostream>
using namespace std;

int main(void) 

{
    double val1 = 0;
    double val2 = 0;
        cin >> val1 >> val2;
    cout << "Cумма = " << val1*val1+val2*val2 << "\n";
    cout << "Разность = " << val1*val1-val2*val2 << "\n";
    cout << "Произведение = " << val1*val1*val2*val2 << "\n";
    cout << "Частное = " << (val1*val1)/(val2*val2) << "\n";
    
    return 0;
}

------------------------------------------------------------------------------------------------------------

2-ой Пример решения.
#include <stdio.h>
 
int main(void)
{
    float a, b;
    printf("a:");
    scanf ("%f", &a);
 
    printf("b:");
    scanf ("%f", &b);
 
    printf("%f\n",a*a+b*b);
    printf("%f\n",a*a-b*b);
    printf("%f\n",a*a*b*b);
    printf("%f\n",(a*a)/(b*b));
 
    return 0;
}

