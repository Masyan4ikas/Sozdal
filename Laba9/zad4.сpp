#include <iostream>
using namespace std;

int main(){
    setlocale(0,"rus");
    int a=0,b=0,c=0,s1=0,s2=0;
    cout << "Введите значения A, B, C через пробел: ";
    cin >> a >> b >> c;
    s1=a*b;
    s2=c*c;
    if (c > a or c > b){
        cout << "Количество квадратов размещенных на прямоугольнике: " << 0 << "\n";
        cout << "Площадь незанятой части прямоугольника: " << s1; 
    }
    else{
        cout << "Количество квадратов размещенных на прямоугольнике: " << s1/s2 << "\n";
        cout << "Площадь незанятой части прямоугольника: " << s1 - (s1/s2)*s2;
    }
}
